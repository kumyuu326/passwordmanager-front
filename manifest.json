{
    "name":"パスワードマネージャー",
    "description":"拡張機能の説明",
    "version":"1.0.0",
    "manifest_version":3,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "permissions":[
        "tabs",
        "activeTab",
        "storage"
    ],
    "host_permissions":[
        "<all_urls>"
    ]
}